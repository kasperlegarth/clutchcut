{
  "name": "clutchcut-desktop",
  "version": "0.0.3",
  "description": "Clutch Cut â€“ Desktop app til analyse af gameplay (Battlefield MVP)",
  "main": "dist-electron/main/index.js",
  "author": "Clutch Cut Team",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k -n VITE,TSC,ELEC -c blue,magenta,green \"vite dev --port 1337\" \"tsc -p tsconfig.electron.json -w\" \"cross-env NODE_ENV=development ELECTRON_ENABLE_LOGGING=1 ELECTRON_DISABLE_SECURITY_WARNINGS=1 nodemon --watch dist-electron --ext js --delay 300ms --exec electron .\"",
    "build:ui": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build": "npm run build:ui && npm run build:electron",
    "preview": "vite preview",
    "dist": "electron-builder",
    "release:win": "rimraf release && electron-builder --win --publish always",
    "release:beta": "rimraf release && electron-builder --win --publish always --prepackaged release --config.publish.channel=beta",
    "release:all": "rimraf release && electron-builder -mwl --publish always"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "lucide-vue-next": "^0.298.0",
    "pinia": "^2.1.7",
    "reka-ui": "^2.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "electron": "^30.5.1",
    "electron-builder": "^26.0.12",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.7",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "vite": "^5.4.7",
    "vite-plugin-vue-devtools": "^7.0.0",
    "wait-on": "^7.2.0"
  }
}
